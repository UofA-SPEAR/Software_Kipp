            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://description/meshes/base_drum.stl"/>
            </geometry>
        </visual>
    
    </link>

    <joint name="base_joint" type="revolute">
        <parent link="base_link"/>
        <child link="base_drum"/>
        <axis xyz="0 0 1"/>
        <limit lower="0.00" upper="3.14159" velocity="50.0" effort="1000.0"/>
        <origin xyz="0 0 0.03935" rpy="0 0 0"/>
    </joint>

