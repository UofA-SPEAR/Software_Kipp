<robot name="kipps_arm">

    <link name="base_link">
        <visual>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <mesh filename="package://description/meshes/base_link.stl"/>
            </geometry>
        </visual>
    </link>

    <link name="base_drum">
        
